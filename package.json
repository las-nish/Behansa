{
  "name": "behansa",
  "scripts": {
    "linux:setup:behansa": "git pull && mkdir -p ~/.config/nvim/ && mkdir -p ~/.behansa/session && rm -rf ~/.config/nvim/* && cp -r Script/Editor/* ~/.config/nvim/",
    "linux:setup:terminal:wezterm": "git pull && cp -r Script/Terminal/Wezterm/.wezterm.lua ~/.wezterm.lua",
    "linux:setup:neovim:packer": "rm -rf ~/.local/share/nvim/* && git clone --depth 1 https://github.com/wbthomason/packer.nvim ~/.local/share/nvim/site/pack/packer/start/packer.nvim",
    "linux:setup:clean": "rm -rf ~/.cache/nvim/*"
  }
}
